{
  "name": "nuxt-react-microfrontend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "install:all": "concurrently \"yarn install:nuxt-qiankun\" \"yarn install:react\"   \"yarn install:nuxt\"  \"yarn install:vue\"",
    "install:nuxt": "cd nuxt-main && yarn",
    "install:react": "cd react-app-qiankun-sub && yarn",
    "install:vue": "cd vue-cli-subapp && yarn",
    "serve:all": "concurrently \"yarn serve:nuxt-qiankun\" \"yarn serve:react\" \"yarn serve:nuxt\" \"yarn serve:vue\"",
    "serve:nuxt": "cd nuxt-main && yarn dev",
    "serve:react": "cd react-app-qiankun-sub && yarn start",
    "serve:vue": "cd vue-cli-subapp && yarn serve",
    "build:all": "concurrently \"yarn build:nuxt-qiankun\" \"yarn build:react\" \"yarn build:vue\" \"yarn build:nuxt\"",
    "build:nuxt": "cd nuxt-main && yarn build",
    "build:react": "cd react-app-qiankun-sub && yarn build",
    "build:vue": "cd vue-cli-subapp && yarn build"
  },
  "keywords": [],
  "author": "https://github.com/rezasohrabi/",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^5.2.0"
  }
}
